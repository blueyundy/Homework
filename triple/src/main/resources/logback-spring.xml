<?xml version="1.0" encoding="UTF-8"?>
<configuration debug="true">
	<appender name="console" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<Pattern>%5p [%c] %m%n</Pattern>
		</encoder>
	</appender>
	
	<logger name="com.club.mileage.triple" level="DEBUG" appender-ref="console" />

	<logger name="jdbc.sqlonly" level="INFO" appender-ref="console" />
	<logger name="jdbc.resultsettable" level="INFO" appender-ref="console" />
	
	<root level="INFO">
		<appender-ref ref="console" />
	</root>
</configuration>